{
  "name": "regex-generator",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://v8187.github.io/regex-generator",
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar scss/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar scss/ -o public/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "npm-run-all": "^4.1.1",
    "react-scripts": "1.0.13"
  }
}
