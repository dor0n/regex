{
  "name": "regex-generator-ai",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://v8187.github.io/regex-generator",
  "scripts": {
    "build-css": "node-sass-chokidar scss/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar scss/ -o public/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.2",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "sortablejs": "^1.7.0",
    "node-sass-chokidar": "0.0.3"
  },
  "devDependencies": {
    "babel-jest": "^22.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "jest": "^22.0.6",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.0.17",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest_setup"
  }
}
